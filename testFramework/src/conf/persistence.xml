<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="testFrameworkPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>model.Grammage</class>
    <class>model.Client</class>
    <class>model.Devie</class>
    <class>model.Employe</class>
    <class>model.Produit</class>
    <class>model.Achatproduit</class>
    <class>model.Depense</class>
    <class>model.Niveauetude</class>
    <class>model.Format</class>
    <class>model.Specialite</class>
    <class>model.Report</class>
    <class>model.Totalsalaire</class>
    <class>model.Clientfidele</class>
    <class>model.Type</class>
    <class>model.Genre</class>
    <class>model.Sommerestant</class>
    <class>model.Qualite</class>
    <class>model.Historiqueachatproduit</class>
    <class>model.Service</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/imprimerie"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
